<% @kittens.each do |kitten| %>
    <tr>
        <td><%= kitten.name %></td>
        <td><%= kitten.favorite_toy %></td>
        <td><%= kitten.cute %></td>
        <td><%= kitten.age %></td>
        <td><%= link_to 'Show', kitten %></td>
        <td><%= link_to 'Edit', edit_kitten_path(kitten) %></td>
        <td><%= link_to 'Destroy', kitten, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
<% end %>